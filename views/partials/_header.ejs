<!DOCTYPE html>
  <html lang="en">
    <header>
      <h1>TinyApp</h1>

      <% if (user) {%>
        <form method="POST" action="/logout">
          <%= user.email %>
          <button type="submit" class="btn btn-primary">Logout</button>
        </form>

      <% } else { %>
        <a href="/register">Register</a>
        <a href="/login">Login</a>
      <% } %>
    </header>
  </html>